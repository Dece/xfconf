#: ../xfconfd/xfconf-backend-perchannel-xml.c:469
#: ../xfconfd/xfconf-backend-perchannel-xml.c:634
#: ../xfconfd/xfconf-backend-perchannel-xml.c:654
#, c-format
msgid "Property \"%s\" does not exist on channel \"%s\""
